<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <Platforms>AnyCPU;x64</Platforms>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.2.0.2-alpha</Version>
    <Description>High-level functional and verifiable TensorFlow 2.0 API.</Description>
    <Product>Sylvester</Product>
    <Authors>Allister Beharry</Authors>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Copyright>Copyright (c) 2019 2020 by Allister Beharry</Copyright>
    <PackageIcon>numanalysis.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/allisterb/Sylvester.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/allisterb/Sylvester/tree/master/src/Fabrics/tf</PackageProjectUrl>
    <PackageTags>f# fsharp tensorflow type-level math numeric</PackageTags>
    <PackageReleaseNotes>First alpha release. Use FSharp.Core &gt;= 4.3.4</PackageReleaseNotes>
    <AssemblyVersion>0.2.0.2</AssemblyVersion>
    <FileVersion>0.2.0.2</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="graphs\TensorGraph.fs" />
    <Compile Include="graphs\TensorScope.fs" />
    <Compile Include="nodes\Arithmetic.fs" />
    <Compile Include="nodes\LinearAlgebra.fs" />
    <Compile Include="edges\Tensor.fs" />
    <Compile Include="edges\Scalar.fs" />
    <Compile Include="edges\Vector.fs" />
    <Compile Include="edges\Matrix.fs" />
    <Compile Include="edges\NDTensor.fs" />
    <Compile Include="AssemblyInfo.fs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Base\Sylvester.Api.Fs\Sylvester.Api.Fs.fsproj" />
    <ProjectReference Include="..\..\..\Lang\Sylvester.Graphs\Sylvester.Graphs.fsproj" />
    <ProjectReference Include="..\..\..\Lang\Sylvester.Tensors\Sylvester.Tensors.fsproj" />
    <ProjectReference Include="..\Sylvester.tf.Api\Sylvester.tf.Api.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\..\..\Users\Allister\Documents\numanalysis.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="4.3.4" />
  </ItemGroup>

</Project>
